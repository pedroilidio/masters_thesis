@InProceedings{Fattahi2019,
  author    = {Fatemeh Fattahi and Mohammad S. Refahi and Behrouz Minaei-Bidgoli},
  booktitle = {5th Iranian Conference on Signal Processing and Intelligent Systems ({ICSPIS})},
  title     = {Drug-target interaction prediction using Edge2vec Algorithm on the heterogeneous network via {SVM}},
  year      = {2019},
  month     = {dec},
  publisher = {{IEEE}},
  comment   = {A predição da interação proteína-alvo de proteína (DTI) é considerada uma etapa fundamental no processo de descoberta de drogas. Os métodos experimentais nesse domínio, apesar de terem resultados precisos e benéficos, consomem muito tempo. Nos últimos anos, esses métodos computacionais não foram superados apenas para esses problemas, mas os resultados deles foram extremamente eficientes. Um tipo dessas técnicas é baseado na previsão de redes em redes biomédicas. Hoje, diferentes abordagens baseadas em rede foram sugeridas para identificar as relações entre medicamentos e alvos que funcionam bem em redes homogêneas. Mas essas abordagens não consideram a semântica de ponta na rede. Nesse papel, empregamos o algoritmo edge2vec para incorporação de nós na rede heterogênea e, em seguida, pelo uso do método de máquina de vetores de suporte para prever interações entre medicamentos e alvos. Nosso método obteve uma precisão significativa do que os métodos existentes.},
  doi       = {10.1109/icspis48872.2019.9066013},
  file      = {Original:Artigos/19 - 10.1109@ICSPIS48872.2019.9066013.pdf:PDF;Traduzido:Artigos/19 - 10.1109@ICSPIS48872.2019.9066013.en.pt.pdf:PDF},
  groups    = {SVM, Classificado, DTI},
}

@InProceedings{Nasution2019,
  author    = {Ahmad Kamal Nasution and Sony Hartono Wijaya and Wisnu Ananta Kusuma},
  booktitle = {2019 International Conference on Advanced Computer Science and information Systems ({ICACSIS})},
  title     = {Prediction of Drug-Target Interaction on Jamu Formulas using Machine Learning Approaches},
  year      = {2019},
  month     = {oct},
  publisher = {{IEEE}},
  comment   = {Jamu é um medicamento herbal indonésio que tem muitos benefícios. A predição de interações medicamentosas-alvo na fórmula de Jamu, utilizando uma abordagem baseada em gráficos, foi realizada, mas os resultados foram insatisfatórios com a área sob a curva de precisão-recall (AUPR) de 0,70. Este estudo desenvolve um modelo de previsão de interações medicamentosas-alvo com a abordagem de aprendizado de máquina usando o Support Vector Machine (SVM) e a Random Forest (RF). O conjunto de dados usado neste estudo é o mesmo que o da pesquisa anterior, obtido do Indonesian Jamu Herbs (IJAH) Analytics. O conjunto de dados representa interações de compostos e proteínas, incluindo marcadores para indicar os de interações. A Análise de Componentes Principais (PCA) é usada como redução de recurso no estágio de pré-processamento. Os modelos de previsão usando SVM e RF combinados com PCA obtêm os melhores resultados AUPR de 0,99.},
  doi       = {10.1109/icacsis47736.2019.8979795},
  groups    = {SVM, Classificado, DTI, Random Forest},
}

